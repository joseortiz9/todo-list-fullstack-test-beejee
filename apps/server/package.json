{
   "name": "@todo-list-fullstack-test/server",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "prepare": "ts-patch install -s && pnpm prisma:init",
      "prisma:init": "rm -rf .prisma && prisma generate",
      "dev": "pnpm prisma:init && cross-env NODE_ENV=development nodemon ./src/server.ts",
      "start": "pnpm prisma:init && cross-env NODE_ENV=production node dist/src/server.js",
      "pm2:start": "pnpm pm2:delete && pnpm build && cross-env NODE_ENV=test pm2 start dist/src/server.js --name @todo-list-fullstack-test/server",
      "pm2:delete": "pm2 delete all &",
      "pm2:logs": "pm2 logs",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "ts:check": "tsc -b",
      "check": "pnpm lint && pnpm ts:check",
      "db:init": "./start-database.sh",
      "build": "tsc -b"
   },
   "dependencies": {
      "@prisma/client": "^5.10.2",
      "@trpc/client": "^10.45.1",
      "@trpc/server": "^10.45.1",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "express": "^4.18.3",
      "helmet": "^7.1.0",
      "hpp": "^0.2.3",
      "iron-session": "^8.0.1",
      "jsonwebtoken": "^9.0.2",
      "prisma": "^5.10.2",
      "superjson": "^2.2.1",
      "zod": "^3.22.4"
   },
   "devDependencies": {
      "@swc/core": "^1.4.2",
      "@todo-list-fullstack-test/shared-ts": "workspace:*",
      "@todo-list-fullstack-test/tsconfig": "workspace:*",
      "@trivago/prettier-plugin-sort-imports": "^4.3.0",
      "@types/bcrypt": "^5.0.2",
      "@types/body-parser": "^1.19.5",
      "@types/cookie-parser": "^1.4.7",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/express-serve-static-core": "^4.17.43",
      "@types/fs-extra": "^11.0.4",
      "@types/hpp": "^0.2.6",
      "@types/jsonwebtoken": "^9.0.6",
      "@types/node": "^18.16.17",
      "@types/qs": "^6.9.12",
      "cross-env": "^7.0.3",
      "dotenv": "^16.4.5",
      "envalid": "^7.3.1",
      "fs-extra": "^11.2.0",
      "nodemon": "^2.0.22",
      "pm2": "^5.3.1",
      "ts-node": "^10.9.2",
      "ts-patch": "^3.1.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.3.3",
      "typescript-transform-paths": "^3.4.7"
   }
}
